{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "cargo build with setcap",
			"type": "shell",
			"command": "cargo build && sudo setcap 'cap_bpf,cap_perfmon,cap_sys_admin+eip' target/debug/edbgserver",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "cargo build"
		}
	]
}